{
  "name": "changesets-release-it-plugin",
  "version": "0.0.0",
  "description": "Plugin to facilitate integration of changesets with release-it",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "bron test.js",
    "release": "release-it"
  },
  "keywords": [
    "release",
    "release-it",
    "release-it-plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abeforgit/changesets-release-it-plugin"
  },
  "homepage": "https://github.com/abeforgit/changesets-release-it-plugin#readme",
  "bugs": "https://github.com/abeforgit/changesets-release-it-plugin/issues",
  "author": "abeforgit",
  "dependencies": {
    "@manypkg/get-packages": "^2.2.0",
    "mdast-util-to-string": "^4.0.0",
    "remark-parse": "^10.0.2",
    "remark-stringify": "^10.0.3",
    "unified": "^11.0.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "bron": "^2.0.3",
    "release-it": "^15.2.0",
    "sinon": "^15.0.1"
  },
  "peerDependencies": {
    "@changesets/cli": "^2.26.0",
    "release-it": "^15.2.0"
  },
  "engines": {
    "node": ">=18.0"
  },
  "release-it": {
    "hooks": {
      "before:init": "npm test"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "./index.js": {}
    }
  }
}
